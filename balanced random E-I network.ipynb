   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "#set parameters\n",
    "dt=1e-4     #s\n",
    "time=10     #s\n",
    "K=100       #number of input neurons\n",
    "K_root=10\n",
    "r_x=10      #Hz\n",
    "V_th=1\n",
    "tau=0.02   #s\n",
    "w=1.5\n",
    "p=r_x*dt\n",
    "t=np.arange(0,time,dt)\n",
    "\n",
    "#initialize Sj,Si and V array\n",
    "S_j_E = np.zeros([K,len(t)])\n",
    "S_j_I = np.zeros([K,len(t)])\n",
    "for j in range(0,K):\n",
    "    S_j_E[j,:]=Poisson_spikes_trains(p,t)\n",
    "    S_j_I[j,:]=Poisson_spikes_trains(p,t)\n",
    "V=np.zeros(len(t))\n",
    "S_i=np.zeros(len(t))\n",
    "\n",
    "#initialize spikes counting\n",
    "spikes_number=0\n",
    "#initialize window\n",
    "len_win=0.1  #s\n",
    "num_win=int(time/len_win)\n",
    "numspikes_in_window=np.zeros(num_win)\n",
    "\n",
    "for k in range(0,len(t)-1):\n",
    "    if(V[k]>=V_th):\n",
    "        S_i[k]=1\n",
    "        V[k+1]=0\n",
    "        spikes_number+=1\n",
    "        spike_which_window=int((k+1)*dt/len_win)\n",
    "        numspikes_in_window[spike_which_window]+=1\n",
    "    else:\n",
    "        total=np.sum(S_j_E[:,k])-np.sum(S_j_I[:,k])\n",
    "        V[k+1]=V[k]+dt*((-V[k]/tau)+(w*total)/(K_root*dt))\n",
    "    \n",
    "if spikes_number!=0:\n",
    "    print(\"Output firing rate =\",spikes_number/time)\n",
    "    miu= np.mean(numspikes_in_window)\n",
    "    sigma_2= np.var(numspikes_in_window)\n",
    "    fano=sigma_2/miu\n",
    "    print('Fano factor =',fano)\n",
    "else:\n",
    "    print(\"Output firing rate = 0\\nFano factor = nan\")"
   ]
