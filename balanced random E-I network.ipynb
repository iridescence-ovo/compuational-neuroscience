   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import random\n",
    "\n",
    "#set parameters\n",
    "dt=1e-4     #s\n",
    "time=2      #s\n",
    "N=1000      #number of neurons in each population\n",
    "K=100       #number of presynaptic partners\n",
    "K_root=np.sqrt(K)\n",
    "r_x=10      #Hz\n",
    "V_th=1\n",
    "tau=0.02   #s\n",
    "\n",
    "#synaptic strength\n",
    "J_EE=1\n",
    "J_IE=1\n",
    "J_EI=-2.5\n",
    "J_II=-2\n",
    "J_EX=2\n",
    "J_IX=1\n",
    "\n",
    "p=dt*r_x\n",
    "t=np.arange(0,time,dt)\n",
    "\n",
    "#initialize Sj,Si and V array\n",
    "S_j_E=np.zeros([N,len(t)])\n",
    "S_j_I=np.zeros([N,len(t)])\n",
    "S_j_X=np.zeros([N,len(t)])\n",
    "for j in range(0,N):\n",
    "    S_j_X[j,:]=Poisson_spikes_trains(p,t)\n",
    "\n",
    "V_E=np.zeros([N,len(t)])\n",
    "V_I=np.zeros([N,len(t)])\n",
    "\n",
    "#initialize connection matrix\n",
    "C_EE=generate_connection(N,K)\n",
    "C_EI=generate_connection(N,K)\n",
    "C_EX=generate_connection(N,K)\n",
    "C_IE=generate_connection(N,K)\n",
    "C_II=generate_connection(N,K)\n",
    "C_IX=generate_connection(N,K)\n",
    "\n",
    "#start simulation\n",
    "for k in range(0,len(t)-1):\n",
    "    spiking_index_E=np.where(V_E[:,k]>V_th)\n",
    "    V_E[spiking_index_E,k]=0\n",
    "    S_j_E[spiking_index_E,k]=1\n",
    "    spiking_index_I=np.where(V_I[:,k]>V_th)\n",
    "    V_I[spiking_index_I,k]=0\n",
    "    S_j_I[spiking_index_I,k]=1\n",
    "    S_EE=S_j_E[:,k]@C_EE\n",
    "    S_EI=S_j_I[:,k]@C_EI\n",
    "    S_EX=S_j_X[:,k]@C_EX\n",
    "    S_IE=S_j_E[:,k]@C_IE\n",
    "    S_II=S_j_I[:,k]@C_II\n",
    "    S_IX=S_j_X[:,k]@C_IX\n",
    "    V_E[:,k+1]=V_E[:,k]-dt*V_E[:,k]/tau+(J_EE*S_EE)/K_root+(J_EI*S_EI)/K_root+(J_EX*S_EX)/K_root\n",
    "    V_I[:,k+1]=V_I[:,k]-dt*V_I[:,k]/tau+(J_IE*S_IE)/K_root+(J_II*S_II)/K_root+(J_IX*S_IX)/K_root\n",
    "\n",
    "#counting spikes and calculate frequency\n",
    "spikes_number_E=S_j_E.sum()\n",
    "spikes_number_I=S_j_I.sum()\n",
    "r_E=spikes_number_E/(N*time)\n",
    "r_I=spikes_number_I/(N*time)\n",
    "\n",
    "print('r_E=',r_E,', r_I=', r_I)"
   ]
