    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "#set parameters\n",
    "dt = 1e-4   #s\n",
    "time=2      #s\n",
    "r_x=10      #Hz\n",
    "V_th=1\n",
    "tau=20e-3   #s\n",
    "w=0.9\n",
    "p=r_x*dt\n",
    "t=np.arange(0,time,dt)\n",
    "\n",
    "#generate one spike train\n",
    "S_j=Poisson_spikes_trains(p,t)\n",
    "\n",
    "V=np.zeros(len(t))\n",
    "S_i=np.zeros(len(t))\n",
    "\n",
    "for k in range(0,len(t)-1):\n",
    "    if(V[k]>=V_th):\n",
    "        V[k+1]=0\n",
    "        S_i[k]=1\n",
    "    else:\n",
    "        V[k+1]=V[k]+dt*((-V[k]/tau)+(w*S_j[k])/dt)\n",
    "    \n",
    "plt.subplot(2,1,1)\n",
    "plt.plot(t,V)\n",
    "plt.title('time varying membrane potential Vi') \n",
    "plt.xlabel('t')\n",
    "plt.ylabel('Vi')\n",
    "\n",
    "plt.subplot(2,1,2)\n",
    "plt.scatter(t,S_j,label='input')\n",
    "plt.scatter(t,S_i*2,label='output')\n",
    "plt.title('input and output spike train') \n",
    "plt.xlabel('t')\n",
    "plt.legend()\n",
    "#cut all the zero values from array S\n",
    "plt.ylim(0.5,2.5)\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
